#! /usr/bin/env node

const { BASE_PATH } = require("../lib/constants.js");
const { getToken } = require("../lib/token.js");

const express = require("express");
const app = express();
app.use(require("../lib/agent.js"));

app.listen(10001, () => {
  console.log(
    `Started nullboard agent. Token is ${getToken()}. Backup directory is at ${BASE_PATH}. Listening on port 10001...`
  );
});
